# Azure PostgreSQL connection parameters
AZURE_POSTGRES_HOST=your-server.postgres.database.azure.com
AZURE_POSTGRES_USER=your-username
AZURE_POSTGRES_PASSWORD=your-password
AZURE_POSTGRES_DB=your-database
AZURE_POSTGRES_SSL_MODE=require

# Azure PostgreSQL replication parameters
AZURE_POSTGRES_PRIMARY_HOST=your-server.postgres.database.azure.com
AZURE_POSTGRES_PRIMARY_USER=your-username
AZURE_POSTGRES_PRIMARY_PASSWORD=your-password
AZURE_POSTGRES_PRIMARY_DB=your-database
AZURE_POSTGRES_PRIMARY_SERVER_NAME=your-server
AZURE_POSTGRES_REPLICA_HOST=your-replica-server.postgres.database.azure.com
AZURE_POSTGRES_REPLICA_USER=your-username
AZURE_POSTGRES_REPLICA_PASSWORD=your-password
AZURE_POSTGRES_REPLICA_DB=your-replica-database
AZURE_POSTGRES_REPLICA_SERVER_NAME=your-replica-server

# Azure Settings
AZURE_SUBSCRIPTION_ID=your-subscription-id
AZURE_RESOURCE_GROUP=your-resource-group